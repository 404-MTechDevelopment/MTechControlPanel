    "compilerOptions": {
        "target": "es2015",
        "module": "esnext",
        "moduleResolution": "node",
        "lib": ["es2015", "dom"],
        "paths": {
            "@/*": ["./src/*"]
        },
        "esModuleInterop": true,
        "skipLibCheck": true
    },
    "exclude": ["node_modules", "dist"]
}
